# libarchive required by cmake
#

PKG="libarchive"
PKG_VERSION="3.2.1 "
PKG_FILE="libarchive-3.2.1.tar.gz"
URL="http://www.libarchive.org/downloads/libarchive-3.2.1.tar.gz"
MD5="afa257047d1941a565216edbf0171e72"


( cat << "xEOFx"

./configure --prefix=/usr --disable-static &&

make



make install

xEOFx
) > tmp
